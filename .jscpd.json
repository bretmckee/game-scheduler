{
  "threshold": 3,
  "reporters": ["console", "json"],
  "ignore": [
    "**/__pycache__/**",
    "**/node_modules/**",
    "**/coverage/**",
    "**/dist/**",
    "**/build/**",
    "**/.venv/**",
    "**/venv/**",
    "**/*.min.js",
    "**/tests/**",
    "**/alembic/versions/**",
    "**/__init__.py"
  ],
  "format": ["python", "typescript", "javascript"],
  "minLines": 6,
  "minTokens": 50,
  "mode": "mild",
  "output": "./.jscpd-report"
}
